{
    "private": false,
    "version": "1.0.54",
    "license": "GPL-3.0-or-later",
    "name": "@clevercanyon/utilities",
    "description": "Another great project by Clever Canyon.",
    "repository": "https://github.com/clevercanyon/utilities",
    "homepage": "https://github.com/clevercanyon/utilities#readme",
    "bugs": "https://github.com/clevercanyon/utilities/issues",
    "funding": "https://github.com/sponsors/clevercanyon",
    "keywords": [
        "clevercanyon"
    ],
    "author": {
        "name": "Clever Canyon",
        "url": "https://clevercanyon.com"
    },
    "contributors": [],
    "type": "module",
    "files": [
        "/dist"
    ],
    "bin": {},
    "imports": {},
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/types/index.d.ts"
        },
        "./brand": {
            "import": "./dist/brand.js",
            "require": "./dist/brand.cjs",
            "types": "./dist/types/brand.d.ts"
        },
        "./cookie": {
            "import": "./dist/cookie.js",
            "require": "./dist/cookie.cjs",
            "types": "./dist/types/cookie.d.ts"
        },
        "./crypto": {
            "import": "./dist/crypto.js",
            "require": "./dist/crypto.cjs",
            "types": "./dist/types/crypto.d.ts"
        },
        "./env": {
            "import": "./dist/env.js",
            "require": "./dist/env.cjs",
            "types": "./dist/types/env.d.ts"
        },
        "./http": {
            "import": "./dist/http.js",
            "require": "./dist/http.cjs",
            "types": "./dist/types/http.d.ts"
        },
        "./obj": {
            "import": "./dist/obj.js",
            "require": "./dist/obj.cjs",
            "types": "./dist/types/obj.d.ts"
        },
        "./react": {
            "import": "./dist/react.js",
            "require": "./dist/react.cjs",
            "types": "./dist/types/react.d.ts"
        },
        "./str": {
            "import": "./dist/str.js",
            "require": "./dist/str.cjs",
            "types": "./dist/types/str.d.ts"
        },
        "./time": {
            "import": "./dist/time.js",
            "require": "./dist/time.cjs",
            "types": "./dist/types/time.d.ts"
        },
        "./url": {
            "import": "./dist/url.js",
            "require": "./dist/url.cjs",
            "types": "./dist/types/url.d.ts"
        }
    },
    "sideEffects": [
        "./src/*.{html,scss,css,tsx,ts,jsx,mjs,js}"
    ],
    "module": "./dist/index.js",
    "main": "./dist/index.cjs",
    "browser": "",
    "unpkg": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        ">=3.1": {
            "./*": [
                "./dist/types/*"
            ]
        }
    },
    "peerDependencies": {
        "crypto-js": "^4.1.1",
        "ignore": "^5.2.1",
        "locutus": "^2.0.16",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "micromatch": "npm:@clevercanyon/micromatch.fork@^4.0.5"
    },
    "peerDependenciesMeta": {},
    "devDependencies": {
        "@clevercanyon/skeleton-dev-deps": "*",
        "@cloudflare/workers-types": "^4.20221111.1",
        "@types/crypto-js": "^4.1.1",
        "@types/locutus": "^0.0.6",
        "@types/lodash": "^4.14.191",
        "@types/micromatch": "^4.0.2",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9"
    },
    "overrides": {},
    "cpu": [
        "x64",
        "arm64"
    ],
    "os": [
        "darwin",
        "linux"
    ],
    "engines": {
        "node": "^19.2.0",
        "npm": "^8.19.3"
    },
    "scripts": {
        "preview": "npx vite preview",
        "dev": "npx vite dev --mode=dev",
        "envs:push": "./dev/.files/bin/envs.js push",
        "envs:pull": "./dev/.files/bin/envs.js pull",
        "envs:setup": "./dev/.files/bin/envs.js setup",
        "envs:setup:new": "./dev/.files/bin/envs.js setup --new",
        "build": "npx vite build --mode=prod && npx tsc",
        "build:dev": "npx vite build --mode=dev && npx tsc",
        "build:ci": "npx vite build --mode=ci && npx tsc",
        "build:stage": "npx vite build --mode=stage && npx tsc",
        "build:prod": "npx vite build --mode=prod && npx tsc",
        "wrangler": "CLOUDFLARE_API_TOKEN=${C10N_CLOUDFLARE_TOKEN} npx wrangler",
        "update:dotfiles": "./dev/.files/bin/update.js dotfiles",
        "update:project": "npm run update:dotfiles && ./dev/.files/bin/update.js project",
        "update:project:repos": "npm run update:dotfiles && ./dev/.files/bin/update.js project --repos",
        "update:project:repos:pkgs": "npm run update:dotfiles && ./dev/.files/bin/update.js project --repos --pkgs",
        "update:projects::": "./dev/.files/bin/update.js projects",
        "update:projects::dotfiles": "./dev/.files/bin/update.js projects --run update:dotfiles",
        "update:projects::project": "./dev/.files/bin/update.js projects --run update:project",
        "update:projects::project:repos": "./dev/.files/bin/update.js projects --run update:project:repos",
        "update:projects::project:repos:pkgs": "./dev/.files/bin/update.js projects --run update:project:repos:pkgs"
    },
    "workspaces": [],
    "config": {
        "c10n": {
            "&": {
                "dotfiles": {
                    "lock": []
                },
                "build": {
                    "appType": "cma",
                    "targetEnv": "any"
                }
            }
        }
    }
}