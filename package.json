	"devDependencies" : { },
	"cpu"             : [
		"x64",
		"arm64"
	],
	"os"              : [
		"darwin",
		"linux"
	],
	"engines"         : {
		"node" : "^18.12.1",
		"npm"  : "^8.19.2"
	},
	"scripts"         : {
		"webpack" : "npx webpack --config ./dev/.files/webpack/config.cjs"
	},
	"config"          : {
		"c10n" : {
			"&" : {
				"dotfiles"                : {
					"lock" : [
						"./.npmignore"
					]
				},
				"webpack"                 : {
					"srcDirs" : [
						"./src/envs/any",
						"./src/envs/cfw",
						"./src/envs/node",
						"./src/envs/web"
					]
				},
				"webpack:./src/envs/any"  : {
					"config" : {
						"target" : "es2021"
					}
				},
				"webpack:./src/envs/cfw"  : {
					"config" : {
						"target" : "webworker"
					}
				},
				"webpack:./src/envs/node" : {
					"config" : {
						"target" : "node"
					}
				},
				"webpack:./src/envs/web"  : {
					"config" : {
						"target" : "browserslist"
					}
				}
			}
		}
	}
}
