		"webpack-cli": "^5.0.0"
	},
	"devDependencies": {},
	"cpu": [
		"x64",
		"arm64"
	],
	"os": [
		"darwin",
		"linux"
	],
	"engines": {
		"node": "^18.5.0",
		"npm": "^8.19.2"
	},
	"scripts": {
		"webpack": "npx webpack --config ./dev/.files/webpack/config.cjs"
	},
	"config": {
		"c10n": {
			"&": {
				"dotfiles": {
					"lock": [
						"./.npmignore"
					]
				},
				"webpack": {
					"srcDirs": [
						"./src/envs/any",
						"./src/envs/cfw",
						"./src/envs/node",
						"./src/envs/web"
					]
				},
				"webpack:./src/envs/any": {
					"config": {
						"target": [
							"browserslist:any"
						],
						"output": {
							"library": {
								"type": "commonjs-static"
							}
						}
					}
				},
				"webpack:./src/envs/cfw": {
					"config": {
						"target": [
							"webworker",
							"browserslist:cfw"
						],
						"output": {
							"library": {
								"type": "commonjs-static"
							}
						}
					}
				},
				"webpack:./src/envs/node": {
					"config": {
						"target": [
							"node",
							"browserslist:node"
						],
						"output": {
							"library": {
								"type": "commonjs-static"
							}
						}
					}
				},
				"webpack:./src/envs/web": {
					"config": {
						"target": [
							"web",
							"browserslist:web"
						],
						"output": {
							"library": {
								"type": "commonjs-static"
							}
						}
					}
				}
			}
		}
	}
}
