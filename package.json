		"@clevercanyon/skeleton-dev-deps": "^1.0.103",
		"@cloudflare/workers-types": "^4.20230115.0",
		"@types/crypto-js": "^4.1.1",
		"@types/locutus": "^0.0.6",
		"@types/lodash": "^4.14.191",
		"@types/micromatch": "^4.0.2",
