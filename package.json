		},
		"./crypto": {
			"import": "./dist/crypto.js",
			"require": "./dist/crypto.cjs",
			"types": "./dist/types/crypto.d.ts"
		},
		"./env": {
			"import": "./dist/env.js",
			"require": "./dist/env.cjs",
			"types": "./dist/types/env.d.ts"
		},
		"./http": {
			"import": "./dist/http.js",
			"require": "./dist/http.cjs",
			"types": "./dist/types/http.d.ts"
		},
