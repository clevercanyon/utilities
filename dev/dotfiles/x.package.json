{
	"private"         : true,
	"type"            : "module",
	"version"         : "1.0.0",
	"license"         : "GPL-2.0-or-later",
	"name"            : "@clevercanyon/[?]",
	"repository"      : "github:clevercanyon/[?]",
	"keywords"        : [ "clevercanyon" ],
	"author"          : {
		"name" : "Clever Canyon",
		"url"  : "https://clevercanyon.com"
	},
	"main"            : "./dst/webpack/index.min.js",
	"dependencies"    : { },
	"devDependencies" : {
		"@clevercanyon/utilities" : "*"
	},
	"cpu"             : [ "x64", "arm64" ],
	"os"              : [ "darwin", "linux" ],
	"engines"         : {
		"node" : "^18.5.0",
		"npm"  : "^8.19.2"
	},
	"scripts"         : {
		"webpack" : "npx webpack --config ./dev/.files/webpack/config.cjs"
	},
	"config"          : {
		"c10n" : {
			"&" : {
				"webpack"       : {
					"srcDirs" : [ "./src" ]
				},
				"webpack:./src" : {
					"config" : {
						"target" : [ "browserslist:any" ],
						"output" : { "library" : { "type" : "commonjs-static" } }
					}
				}
			}
		}
	}
}
