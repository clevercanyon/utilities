<? if ($.$build) {
	const fs = $.$build.require('node:fs');
	const path = $.$build.require('node:path');
	const qs = $.$build.require('node:querystring');

	var pngIcon = $.pngIcon || ($.$build.env.APP_BASE_PATH && fs.existsSync(path.resolve($.$build.projDir, './src/cargo/assets/brand/icon.png')) ?
		$.$build.env.APP_BASE_PATH + '/assets/brand/icon.png?' + qs.encode({ v: $.$build.pkg.version || '' })
	: '') || '';
	var svgIcon = $.svgIcon || ($.$build.env.APP_BASE_PATH && fs.existsSync(path.resolve($.$build.projDir, './src/cargo/assets/brand/icon.svg')) ?
		$.$build.env.APP_BASE_PATH + '/assets/brand/icon.svg?' + qs.encode({ v: $.$build.pkg.version || '' })
	: '') || '';
} else {
	var pngIcon = $.pngIcon || '';
	var svgIcon = $.svgIcon || '';
} ?>
<? if (pngIcon) { ?>
	<link rel="icon" href="<?= pngIcon ?>" type="image/png" />
<? } ?>
<? if (svgIcon) { ?>
	<link rel="icon" href="<?= svgIcon ?>" type="image/svg+xml" />
<? } ?>
